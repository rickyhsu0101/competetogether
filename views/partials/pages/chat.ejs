<div class="container chat-container z-depth-3">
  <h2 class="chat-h2"><%= chatName %></h2>
  <div class="chat-msgs">
    <% if (chatMessages.length > 0) {%>
      <% for (var i = 0; i < chatMessages.length; i++) {%>
        <div class="messages">
          <span><b style="letter-spacing: 1px;"><%= chatMessages[i].username %></b>: </span>
          <span><%= chatMessages[i].messageContent %></span>
        </div>
      <%}%>
    <%}%>
  </div>
  <div class="bg-white">
    <!--ADD CHAT ID LOGIC-->
    <textarea name="input" data-name="<%= user.username %>" data-room="<%= chatPage %>" id="send-msg" cols="0" rows="2" placeholder="New Message"></textarea>
  </div>
</div>
