<div class = "container formDiv">
    <div class = "row">
        <form class = "col s12" method = "POST" action= "/api/login">
            <div class="input-field col s12">
                <input placeholder="Username" id="username" type="text" name="username">
                <label for="username">Username</label>
            </div>
            <div class="input-field col s12">
                <input placeholder="Password" id="password" type="password" name="password">
                <label for="password">Password</label>
            </div>
            <button class = "wave-effect waves-light btn submitButton" type = "submit">Sign In</button>
        </form>
    </div>
</div>